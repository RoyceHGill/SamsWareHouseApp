@model SamWarehouseApp.Models.ListDetails

@{
    ViewData["Title"] = "Delete";
}

<h3 class="default-text-color">Are you sure?</h3>
<div>
    
    <div class="col d-flex justify-content-around">
        <button type="button" class="btn btn-primary" data-bs-dismiss="modal" id="close-dialog">Cancel</button>
        <form asp-action="Delete" asp-controller="ListDetails">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" />
        </form>
    </div>
</div>
